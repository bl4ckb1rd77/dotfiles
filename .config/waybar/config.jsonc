[
  {
    "name": "top",
    "include": [
      "$HOME/.config/waybar/modules.jsonc",
      "$HOME/.config/waybar/workspace.jsonc",
      "$HOME/.config/waybar/modules-disks.jsonc"
    ],
    "layer": "top",
    "position": "top",
    "reload_style_on_change": 1,
    "height": 10,
    "margin": 0,
    "spacing": 0,
    "modules-left": [
      "custom/leftstart",
      "custom/startmenu",
      "hyprland/workspaces",
      "custom/terminal",
      "custom/filemanager",
      "custom/browser",
      "custom/steam",
      "custom/discord",
    ],
    "modules-center": [
      "clock",
      "mpris",
    ],
    "modules-right": [
      "cpu",
      "memory",
      "disk#root",
      // "disk#home",
      "disk#steam",
      // "group/hardware",
      "pulseaudio",
      "battery",
      "idle_inhibitor",
      // "custom/theme",
      "custom/updater",
      "systemd-failed-units",
      "tray",
      "custom/notification",
      "custom/powermenu",
      "custom/rightend"
    ],
    "group/hardware": {
      "orientation": "horizontal",
      "drawer": {
        "transition-duration": 500,
        "transition-left-to-right": false
      },
      "modules": [
        "custom/group",
        "cpu",
        "memory",
        "disk#root"
      ]
    }
  }
]
