[
  {
    "name": "top",
    "include": ["$HOME/.config/waybar/modules.jsonc","$HOME/.config/waybar/workspace.jsonc"],
    "layer": "top",
    "position": "top",
    "reload_style_on_change": 1,
    "height": 10,
    "margin": 0,
    "spacing": 0,
    "modules-left": [
      "custom/left",
      "custom/startmenu",
      "hyprland/workspaces",
      "custom/weather",
      "mpris",
      "custom/right",
      "hyprland/window",
    ],
    "modules-center": [
      "clock",
    ],
    "modules-right": [
      "custom/left",
      // "cpu",
      // "memory",
      // "disk#root",
      // "disk#home",
      "group/hardware",
      "custom/updater",
      "systemd-failed-units",
      "custom/touchpad",
      "custom/ipinfo",
      "battery",
      "pulseaudio",
      "pulseaudio/slider",
      "custom/mpdvol",
      "idle_inhibitor",
      "tray",
      "custom/notification",
      "custom/right",
    ],
    "group/hardware": {
  		"orientation": "horizontal",
      "drawer": {
        "transition-duration": 500,
        "transition-left-to-right": false,
      },
      "modules": [
        "custom/group",
        "cpu",
        "memory",
        "disk#root",
        "disk#home",
	  	]
  	},
  }
